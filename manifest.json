{
    "manifest_version": 2,

      "name": "Phone Dork",
      "description": "Highlight a phone number, right-click and execute a search.",
      "version": "0.0.1",

      "browser_action": {
        "default_icon": "gg-icon.png",
        "default_popup": "phonedork.html",
        "default_title": "Click Me"
      },
      "permissions": [
        "activeTab",
        "storage",
        "contextMenus"
      ],
      "icons": {
        "16": "gg-icon.png",
        "48": "gg-icon.png",
        "128": "gg-icon.png"
      },
      "background": {
        "page": "phonedork.html"
      }
}
